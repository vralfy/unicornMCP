{
  "servers": {
    "mcpcli-wsl": {
      "type": "stdio",
      "command": "wsl",
      "args": [
        "npm",
        "run",
        "mcpcli",
        "--prefix=~/git/docker/project/unicornmcp",
      ],
      "env": {
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      }
    },
    "mcpcli": {
      "type": "stdio",
      "command": "npm",
      "args": [
        "run",
        "mcpcli",
        "--prefix=${workspaceFolder}",
      ],
      "env": {
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      }
    }
  }
}