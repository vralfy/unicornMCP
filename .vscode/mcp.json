{
	"servers": {
		"mcpcli-wsl": {
			"type": "stdio",
			"command": "wsl",
			"args": [
				"npm",
				"run",
				"cli",
				"--prefix=~/git/docker/project/unicornmcp"
			],
			"env": {
				"NODE_TLS_REJECT_UNAUTHORIZED": "0"
			}
		},
		"mcpcli": {
			"type": "stdio",
			"command": "npm",
			"args": [
				"run",
				"cli",
				"--prefix=${workspaceFolder}"
			],
			"env": {
				"NODE_TLS_REJECT_UNAUTHORIZED": "0"
			}
		},
		"rainbowroad": {
			"url": "http://localhost:3000/mcp",
			"type": "http"
		},
		"blender": {
      "type": "stdio",
      "command": "wsl",
      "args": [
        "/mnt/c/Users/xcx2440/Documents/git/docker/project/praktikum/.vscode/blender-mcp.zsh"
      ]
    }
	},
	"inputs": []
}